{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },

  "AllowedHosts": "*",

  "DatabaseProviders": {
    "PostgreSqlDatabaseProvider": "PostgreSql",
    "MSSqlDatabaseProvider": "MicrosoftSQLServer"
  },

  "ConnectionStrings": {
    "PostgreSqlConnectionStrings": "",
    "SqlConnectionSectionName": ""
  },

  "ProviderModelOptions": "PostgreSql",

  "DatabaseModelOptions": {
    "MaxRetryCount": 3,
    "MaxRetryDelay": 5,
    "CommandTimeout": 30,
    "EnableDetailedErrors": false,
    "EnableSensitiveDataLogging": false
  },

  "HangfireModelOptions": {
    "UseInMemory": false,
    "MaxRetryCount": 5
  },

  "JwtModelOptions": {
    "SecretKey": "",
    "ExpiryMinutes": 0,
    "Issuer": "",
    "Audience": "",
    "SaveToken": true,
    "ValidateIssuer": true,
    "ValidateAudience": true,
    "ValidateLifetime": true,
    "ValidateIssuerSigningKey": true
  },

  "ApiVersioningModelOptions": {
    "AssumeDefaultVersionWhenUnspecified": true,
    "ApiActualVersion": {
      "majorVersion": 1,
      "minorVersion": 0
    },
    "ReportApiVersions": true,
    "ApiVersionReader": ""
  },

  "CorsModelOptions": {
    "Policy": "Becoming_Cors_Policy"
  },

  "SwaggerModelOptions": {
    "SwaggerTitle": "Becoming - .NET Core (.NET 6) Web API",
    "SchemeReferemceId": "Bearer_Auth",
    "BearerFormat": "JWT",
    "SecuritySchemeName": "Becoming authorization",
    "SchemeDescription": "Put your JWT Bearer token on textbox below!"
  }
}
